// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Report {
  id String @id
  name String
  rating Int
  comment String
  link String
  dateYYYYMMDD String
  imgUrl String
  user User @relation(fields: [userId], references: [id])
  userId String
}

model User {
  id String @id
  name String
  email String @unique

  Report Report[]
}